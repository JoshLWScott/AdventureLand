{
  "compilerOptions": {
    "outDir": "./dist",
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "module": "es2015",
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "sourceMap": false,
    "target": "es5",
    "typeRoots": [
      "./@types/AdventureLand/",
      "./node_modules/@types/pixi.js"
    ]
  },
  "include": ["./src"]
}
